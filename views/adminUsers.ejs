<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Users</title>
</head>
<body>
    <h1>Manage Users</h1>
    <!-- Display users here -->
    <% users.forEach(user => { %>
        <div>
            <h2><%= user.userName %></h2>
            <p>Full Name: <%= user.fName %> <%= user.lName %></p>
            <!-- Add links or forms for edit/delete actions -->
        </div>
    <% }) %>
    <a href="/admin/users/add">Add New User</a>
</body>
</html>
